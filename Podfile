source 'https://github.com/CocoaPods/Specs.git'

xcodeproj 'MobileEdge-iOS.xcodeproj'
# Uncomment this line to define a global platform for your project
platform :ios, "8.1"

target "MobileEdge-iOS" do

pod "AFNetworking",         "~> 2.0"
pod "AFNetworkActivityLogger", "~> 2.0"
pod "CocoaLumberjack",      "~> 1.9.2"
pod "FXKeychain",           "~> 1.5.1"
pod "HKDFKit",              "~> 0.0.3"
pod "SodiumObjc",           "~> 1.1", :git => "https://github.com/r-arias/SodiumObjc.git"
pod "CPAProxy",             "~> 0.1", :git => 'https://github.com/ursachec/CPAProxy.git' 

end

target "MobileEdge-iOSTests" do

pod "AFNetworking",         "~> 2.0"
pod "AFNetworkActivityLogger", "~> 2.0"
pod "CocoaLumberjack",      "~> 1.9.2"
pod "FXKeychain",           "~> 1.5.1"
pod "HKDFKit",              "~> 0.0.3"
pod "SodiumObjc",           "~> 1.1", :git => "https://github.com/r-arias/SodiumObjc.git"
pod "CPAProxy",             "~> 0.1", :git => 'https://github.com/ursachec/CPAProxy.git' 


end

